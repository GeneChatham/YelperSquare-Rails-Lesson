<%= form_for @restaurant do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<br>
	<%= f.label :hours %>
	<%= f.text_field :hours %>
	<br>
	<%= f.label :cuisine %>
	<%= f.text_field :cuisine %>
	<br>
	<%= f.label :address %>
	<%= f.text_field :address %>
	<br>
	<%= f.label :average_rating %>
	<%= f.text_field :average_rating %>
	<br>
	<%= f.label :photo_url %>
	<%= f.text_field :photo_url %>
	<br>
	<%= f.submit %>
<% end %>